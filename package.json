{
  "name": "trust-env",
  "version": "1.3.1",
  "description": "Make process.env usage safe",
  "main": "dist/index.js",
  "repository": "git@github.com:cubyn/trust-env",
  "license": "MIT",
  "scripts": {
    "serve": "node dist/",
    "serve:watch": "nodemon --ext ts --exec 'yarn build --incremental && yarn serve'",
    "build": "./node_modules/.bin/tsc",
    "build:watch": "./node_modules/.bin/tsc -w",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cover": "jest --coverage",
    "lint": "./node_modules/.bin/eslint src --ext .ts",
    "format": "./node_modules/.bin/prettier --write 'src/**/*.ts'"
  },
  "dependencies": {
    "is_js": "^0.9.0"
  },
  "devDependencies": {
    "@types/is": "^0.0.21",
    "@types/jest": "^25.0.0",
    "@types/node": "^14.0.0",
    "@typescript-eslint/eslint-plugin": "^3.0.0",
    "@typescript-eslint/parser": "^3.0.0",
    "eslint": "^7.0.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-jest": "^23.13.0",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-unicorn": "^20.0.0",
    "jest": "^26.0.0",
    "jest-junit": "^10.0.0",
    "nodemon": "^2.0.0",
    "prettier": "^2.0.0",
    "ts-jest": "^26.0.0",
    "typescript": "^3.9.0"
  },
  "keywords": [
    "env",
    ".env",
    "environment",
    "variables",
    "config",
    "settings"
  ]
}
