{
  "name": "trust-env",
  "version": "1.3.1",
  "description": "Make process.env usage safe",
  "main": "dist/index.js",
  "repository": "git@github.com:cubyn/trust-env",
  "license": "MIT",
  "scripts": {
    "serve": "node dist/",
    "serve:watch": "nodemon --ext ts --exec 'yarn build --incremental && yarn serve'",
    "build": "./node_modules/.bin/tsc",
    "build:watch": "./node_modules/.bin/tsc -w",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cover": "jest --coverage",
    "lint": "./node_modules/.bin/eslint src --ext .ts",
    "format": "./node_modules/.bin/prettier --write 'src/**/*.ts'"
  },
  "dependencies": {
    "is_js": "^0.9.0"
  },
  "devDependencies": {
    "eslint": "7.0.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-cubyn": "^2.0.0",
    "eslint-plugin-import": "^2.0.0",
    "eslint-plugin-jest": "^23.0.0",
    "eslint-plugin-unicorn": "^20.0.0",
    "jest": "^26.0.0",
    "jest-junit": "^10.0.0",
    "typescript": "^3.9.3"
  },
  "keywords": [
    "env",
    ".env",
    "environment",
    "variables",
    "config",
    "settings"
  ]
}
